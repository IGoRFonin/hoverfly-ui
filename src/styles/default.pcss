html {
    font-size: 15px;
    font-family: Helvetica, sans-serif;
    line-height: 24px;
}

body {
    overflow-x: hidden;

    color: #333333;
}

button {
    border: none;

    background-color: transparent;
}

ul {
    list-style: none;
}

* {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}
